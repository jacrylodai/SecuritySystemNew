<?xml version="1.0"?>
<!DOCTYPE hibernate-mapping PUBLIC 
	"-//Hibernate/Hibernate Mapping DTD 3.0//EN"
	"http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">
<hibernate-mapping>
	<class name="com.ldp.security.report.domain.SpecialPeopleInfo"
		 table="t_special_people_info">
		<id name="specialPeopleInfoId" column="special_people_info_id">
			<generator class="identity"/>			
		</id>
		
		<many-to-one name="reportUser" column="report_user_id"/>
		<many-to-one name="department" column="department_id"/>
		<many-to-one name="station" column="station_id"/>

		<property name="state" column="state"/>		
		<property name="buyTicketTimeString" column="buy_ticket_time"/>
		<property name="staDateString" column="sta_date"/>		
		<property name="startLocation" column="start_location"/>
		<property name="destination" column="destination"/>
		<property name="trainNumber" column="train_number"/>
		<property name="trainStartTimeString" column="train_start_time"/>		
		<property name="passengerName" column="passenger_name"/>
		<property name="passengerIdentity" column="passenger_identity"/>
		<property name="ticketNumber" column="ticket_number"/>
		<property name="sitNumber" column="sit_number"/>
		<property name="reportTimeString" column="report_time"/>
		
	</class>
	
</hibernate-mapping>